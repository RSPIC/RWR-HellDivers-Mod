    <faction initial_over_capacity="0" ai_accuracy="0.94" capacity_multiplier="0.0" />
  
  </command>
  
  <command
	  class="faction_resources"
	  faction_id="0">
    <vehicle key="radar_truck.vehicle" enabled="0"/>
  </command>
  <command
	  class="faction_resources"
	  faction_id="1">
    <vehicle key="aa_emplacement.vehicle" enabled="1"/>
  </command>  

