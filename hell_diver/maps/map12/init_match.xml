<commands>
  <!-- start_game command: new game menu picks up defaults from here and allows user to override certain settings -->
  <command 
	  class="start_game" 
	  vehicles="1" 
	  max_soldiers="80" 
	  player_bot_compensation="6" 
	  xp_multiplier="1.0"
	  rp_multiplier="1.0"
	  friendly_fire="0" 
	  base_capture_system="any" >

    <faction initial_over_capacity="0" ai_accuracy="0.94" capacity_multiplier="0.4" />
