particle_system p2_peacemaker_hit_01
{
	quota 50
	material diamond_fill_soft_02
	particle_width 0.8
	particle_height 2
	renderer billboard
	billboard_type oriented_self
	billboard_origin center
	billboard_rotation_type vertex
	
	emitter Point
	{
		angle 45
		colour 1 0.3 0 1
		direction 0 1 0
		emission_rate 300
		position 0 0 0
		velocity 100
		time_to_live 0.012
		duration 0.02
	}
	affector Scaler
	{
		rate 0
	}
		affector Rotator
	{
		rotation_speed_range_start	0
		rotation_speed_range_end	0
		rotation_range_start	0
		rotation_range_end	0
	}
	affector ColourFader2
	{
		red1 	100
		green1 	30
		bule1 	0
		alpha1 	100
		red2 	10
		green2	10
		blue2	10
		alpha2	10
		state_change 0.0001
	}
	affector LinearForce{
		force_vector 0 0 0
		force_application add
	}
	affector DirectionRandomiser 
	{
		randomness 0
		scope 0
		keep_velocity false
	}
}
