particle_system p2_peacemaker_activated_ambient_light
{
	quota 30
	material point_soft_edge_100_pass
	particle_width 15
	particle_height 15
	renderer billboard
	billboard_rotation_type vertex
	iteration_interval 0.01
	emitter Box
	{
		angle 360
		colour 0.02 0.012 0.0 0.05
		direction 0 1 0
		emission_rate 50
		position 0 0 0
		velocity_min 0.1
		velocity_max 0.3
		time_to_live 0.05
		duration 1
		width 3
		height 3
		depth 1
		
	}
	affector Scaler
	{
		rate 30
	}
