--------------------------------------material-----------------------------------
material alpha_blend_base_real{
	receive_shadows off
	transparency_casts_shadows on
	technique{
		pass{
			lighting off
			fog_override true
			depth_write off
			scene_blend alpha_blend
		}
	}
}
material add_base_real{
	receive_shadows off
	technique{
		pass{
			lighting off
			fog_override true
			depth_write off
			scene_blend add
		}
	}
}
material diy_base{
	receive_shadows off
	technique{
		pass{
			lighting off
			fog_override true
			depth_write off
		}
	}
}
