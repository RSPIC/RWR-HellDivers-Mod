particle_system real_smoke_1
{
	quota 350
	material Tex_Smoke17C_real
	particle_width 7
	particle_height 10
	renderer billboard
	billboard_type point
	billboard_origin center
	billboard_rotation_type vertex
	common_direction 0 0.3 1
	common_up_vector 0 1 1
	iteration_interval 0
	
	emitter Point
	{
		angle 10
		colour 1 1 1 1
		direction 0 1 0
		emission_rate 20
		position 0 4 0
		velocity_min 0.001
		velocity_max 0.1
		time_to_live 3.5
		duration 0.1
	}
	affector Scaler
	{
		rate 2
	}
		affector Rotator
	{
		rotation_speed_range_start	2
		rotation_speed_range_end	-2
		rotation_range_start	0
		rotation_range_end	0
	}
	affector ColourInterpolator
	{
		time0 0
		colour0 0.4 0.4 0.4 1
		time1 0.5
		colour1 0.3 0.3 0.3 1
		time2 1
		colour2 1 1 1 0
	}
	affector LinearForce{
		force_vector 0 0.3 0
		force_application add
	}
}
