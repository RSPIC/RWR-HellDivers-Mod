----------------------material----------------------
material alpha_blend_base{
	receive_shadows off
	technique{
		pass{
			lighting off
			fog_override true
			depth_write off
			scene_blend alpha_blend
		}
	}
}
material add_base{
	receive_shadows off
	technique{
		pass{
			lighting off
			fog_override true
			depth_write off
			scene_blend add
		}
	}
}

material icon_phoenix_01	:	alpha_blend_base
{
	technique{
		pass{
			texture_unit 
			{
				rotate 180
				texture icon_phoenix_01.png 
			}
		}
	}
}
	
material lens_circle_03	: add_base		//内光圈虚化
{
    technique
    {
        pass
        {
            texture_unit
            {
                texture lens_circle_03.png
            }
        }
    }
}
material point_soft_edge_100	:	add_base
{technique{
		pass{
			texture_unit{
	  texture point_soft_edge_100.png 
}}}}
material re_explosion_fire_type01 : alpha_blend_base
{	technique{
		pass{
			lighting on
			ambient 1 1 1 1
			emissive 1 1 1 1
		texture_unit{
		anim_texture exposion_fire_type01.png 6 0.5
		tex_address_mode clamp
		filtering biliner
}}}}
material pump_smoke_01 :	alpha_blend_base
{
	technique {
		pass {
			texture_unit {
				texture pump_smoke_01.png 	
			}
		}
	}
}
material hollow_blast_smoke_01 	//空心爆炸溅射烟雾
{
	receive_shadows off
    technique
    {
        pass
        {
            lighting off
            scene_blend add
            depth_write off
			fog_override true
            texture_unit
            {
                texture hollow_blast_smoke_01.png
            }
        }
    }
}
material re_point_soft_edge_100 
{
	receive_shadows off
    technique
    {
        pass
        {
            lighting on
			ambient 0.2 0.2 0.2 0.2
			emissive 0.5 0.1 0 0.1
            scene_blend add
            depth_write off
			fog_override true
            texture_unit
            {
                texture point_soft_edge_100.png
            }
        }
    }
}
