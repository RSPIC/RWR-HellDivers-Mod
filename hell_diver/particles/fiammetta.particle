
particle_system fiammetta_hit_ground_anim_fire_01
{
	quota 300
	material re_explosion_fire_type01
	particle_width 2
	particle_height 2
	renderer billboard
	billboard_type point
	billboard_rotation_type vertex
	emitter Ring{
		angle 90
		colour 0.1 0.3 0 1
		direction 0 1 0
		emission_rate 30
		position 0 1 0
		velocity_min 0.5
		velocity_max 1
		time_to_live 0.55
		duration 0.3
		width 6
		height 6
		depth 1
		inner_width	0.6
		inner_height 0.6
	}
	affector ColourFader2{
		red1	0
		green1  0
		blue1	0
		alpha1	0
		red2	-2
		green2  -2
		blue2	-2
		alpha2	-2
		state_change 0.3
	}
		affector Rotator{
		rotation_speed_range_start	90
		rotation_speed_range_end	90
		rotation_range_start	0
		rotation_range_end	360
	}
		affector DirectionRandomiser
	{
		randomness 5
		scope 100
		keep_velocity true
	}
}


