particle_system ring_spread_smoke_apocalypse_tank		//地面环形烟雾
{
	quota 500
	material explosion_smoke_03
	particle_width 2
	particle_height 2
	renderer billboard
	billboard_rotation_type vertex
	billboard_type point
	emitter Ring
	{
		angle 90
		colour 0 0.3 1 1
		direction 0 1 0
		emission_rate 300
		position 0 0 0
		velocity 0.001
		velocity_min 0.001
		velocity_max 0.001
		time_to_live 1
		time_to_live_min 0.2
		time_to_live_max 0.3
		duration 0.15
		width 2.5
		height 2.5
		depth 0.5
		inner_width 0.1
		inner_height 0.1
	}

	affector Scaler
	{
		rate 10
	}
	affector ColourFader2
	{
		red1	3
		green1  3
		blue1	3
		alpha1	3
		red2	 -5
		green2  -5
		blue2	-5
		alpha2	-5
		state_change 0.1
	}
	affector Rotator
	{
		rotation_speed_range_start	-45
		rotation_speed_range_end	45
		rotation_range_start	0
		rotation_range_end	360
	}
}
