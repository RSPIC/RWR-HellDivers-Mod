material explosion_smoke_03 	//高透明度爆炸烟雾
{
	receive_shadows off
    technique
    {
        pass
        {
            lighting off
            scene_blend add
            depth_write off
			fog_override true
            texture_unit
            {
                texture explosion_smoke_03.png
            }
        }
    }
}
