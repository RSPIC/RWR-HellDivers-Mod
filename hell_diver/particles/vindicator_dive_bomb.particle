particle_system blast_smoke_cross	//十字爆炸烟雾
{
	quota 1000
	material blast_smoke_cross
	particle_width 4
	particle_height 4
	renderer billboard
	billboard_rotation_type vertex
	billboard_type point
	
	emitter Ring
	{
		angle 0
		colour 1 0.3 0 0.1
		direction 0 1 0
		emission_rate 40
		position 0 0 0
		velocity 0.1
		time_to_live 0.1
		duration 0.2
		width 4
		height 4
		depth 1
		inner_width 2
		inner_height 2
	}
	
	affector ColourFader
	{
		red	 0.1
		green	-0.2
		blue	-0.1
		alpha	-1
	}
	affector Scaler
	{
		rate 60
	}
	
	affector Rotator
	{
		rotation_speed_range_start -15
		rotation_speed_range_end 15
		rotation_range_start 0
		rotation_range_end 30
	}
}

