particle_system spread_line	//点扩散冲击线性光
{
	quota 300
	material spread_line
	particle_width 0.15
	particle_height 4
	renderer billboard
	billboard_rotation_type vertex
	billboard_type oriented_self
	
	emitter Point
	{
		angle 35
		colour 1 1 0 1
		direction 0 -1 0
		emission_rate 200
		position 0 0 0
		velocity -50
		velocity_min -20
		velocity_max -35
		time_to_live 0.8
		time_to_live_min 0.3
		time_to_live_max 0.5
		duration 0.1
	}
	affector ColourInterpolator
	{
		time0 0
		colour0 1 1 0 1
		time1 0.7
		colour1 1 1 0 1
		time2 1
		colour2 0 0 0 0
	}
	affector LinearForce
	{
		force_vector 0 -1 0
		force_application add
	}
	affector Scaler
	{
		rate 0
	}
}

particle_system blast_smoke_Vindicator	//普通扩散烟雾
{
	quota 3000
	material spread_smoke
	particle_width 1.5
	particle_height 1.5
	cull_each false
	renderer billboard
	local_space false
	billboard_type point
	common_direction 0 1 0
	billboard_rotation_type vertex
	
	emitter Ring
	{
		angle 90
		colour 1 1 1 1
		colour_range_start 1 1 1 1
		colour_range_end 1 1 1 1
		direction 0 1 0
		emission_rate 200
		position 0 1 0
		velocity 1
		time_to_live 1.5
		time_to_live_min 1
		time_to_live_min 1.5
		duration 0.3
		width 5
		height 5
		depth 1
		inner_width 1
		inner_height 1
	}
	affector Scaler
	{
		rate 1
	}
	affector ColourFader
	{
		red	 1
		green	0.8
		blue	0.8
		alpha	-1
	}
	affector Rotator
	{
		rotation_speed_range_start -60
		rotation_speed_range_end 60
		rotation_range_start 0
		rotation_range_end 30
	}
}

-----------------------------------材质----------------------------
material point_light_area
{
	receive_shadows off
    technique
    {
        pass
        {
            lighting off
			emissive 1 1 1 1
            scene_blend add
            depth_write off
			fog_override true
			
            texture_unit
            {
                texture point_light_area.png
            }
        }
    }
}

