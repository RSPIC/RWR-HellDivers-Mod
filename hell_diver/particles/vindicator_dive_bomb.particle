particle_system point_light_area	//地面点光源
{
	quota 1000
	material point_light_area
	particle_width 4
	particle_height 4
	renderer billboard
	billboard_rotation_type vertex
	billboard_type oriented_self
	common_direction 0 0 1
	common_up_vector 1 0 0
	
	emitter Point
	{
		angle 45
		colour 1 0.3 0 0.00001
		direction 0 1 0
		emission_rate 40
		position 0 0.3 0
		velocity 0.1
		time_to_live 1
		duration 1
	}
	
	affector ColourFader
	{
		red	 0.1
		green	-0.2
		blue	-0.1
		alpha	-1
	}
	
	
	affector Scaler
	{
		rate -1
	}
	
}

