//紫色标准
particle_system mg4_blast_shine_center_hollow		//高射速枪中心中空爆炸辉光
{
	quota 1000
	material hollow_blast_smoke_01
	particle_width 0.1
	particle_height 0.1
	renderer billboard
	billboard_rotation_type vertex
	billboard_type point
	
	emitter Point
	{
		angle 0
		colour 1 0 1 1
		direction 0 1 0
		emission_rate 30
		position 0 0 0
		velocity 0
		time_to_live 0.12
		duration 0.05
	}
	
	affector ColourFader2
	{
		red1	3
		green1	3
		blue1	3
		alpha1	3
		red2	-1
		green2	-10
		blue2	-1
		alpha2	-10
		state_change 0.02
	}
	affector Scaler
	{
		rate 30
	}
	affector Rotator
	{
		rotation_speed_range_start -60
		rotation_speed_range_end 60
		rotation_range_start 0
		rotation_range_end 360
	}
}
