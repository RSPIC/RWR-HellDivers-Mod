    affector ColourInterpolator
	{
		time0 0
		colour0 1 1 1 1
		time1 0.2
		colour1 1 0.3 0.5 1
		time2 0.7s
		colour2 0.3 0.3 0.3 1
		time3 1
		colour3 0 0 0 0
	}
	emitter Box
	{
		angle 5
		colour 0 0 0 0
		direction 0 1 0
		emission_rate 150
		position 0 0 0
		velocity_min 0.1
		velocity_max 0.5
		time_to_live 1.0
		duration 0.1
        width 4
		height 4
		depth 0.1
	}
	affector Scaler
	{
		rate 1
	}
