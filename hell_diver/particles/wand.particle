	quota 50
	material wand_blast_laser
	particle_width 20
	particle_height 80
	renderer billboard	
	billboard_rotation_type vertex
	billboard_origin bottom_center
	billboard_type oriented_common
	common_direction 0 1 0
	common_up_vector 0 0 1
    emitter Point{
        angle 0
		colour 0 0 0 0
		direction 0 1 0
		emission_rate 100
		position 0 0 0
		velocity 0.001
		time_to_live 3
		duration 0.2
    }
    affector Scaler{rate -10}
    affector ColourFader2{
		red1	0.3
		green1  0.3
		blue1	0.3
		alpha1	0.3
		red2	 0
		green2  0
		blue2	0
		alpha2	0
		state_change 2.5
	}
}
