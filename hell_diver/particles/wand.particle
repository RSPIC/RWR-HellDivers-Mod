particle_system wand_gather_wave_01     //汇聚光效暗光		
{
	quota 60
	material gather_wave_01
	particle_width 30
	particle_height 30
	renderer billboard	
	billboard_rotation_type vertex
	billboard_origin center
	billboard_type oriented_common
    common_direction 0 1 1
	emitter Point{
		angle 0
        colour 0 0 0 0
		direction 0 1 0
		emission_rate 40
		position 0 0 0
		velocity 0.001
		time_to_live 1
		duration 12
	}
	affector Scaler{
		rate -30
	}
	affector Rotator{
		rotation_speed_range_start	15
		rotation_speed_range_end	18
		rotation_range_start	0
		rotation_range_end	0
	}
    affector ColourFader2{
        red1	0.8
		green1  0.8
		blue1	0.8
		alpha1	1
		red2	 -1
		green2  -1
		blue2	-1
		alpha2	-1
		state_change 0.3
    }
    
}

