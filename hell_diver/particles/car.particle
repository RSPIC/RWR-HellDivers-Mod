particle_system car_slide_spary_spark
{
	quota 500
	material point_hard_edge
	particle_width 0.1
	particle_height 0.22
	renderer billboard
	billboard_type oriented_self
	billboard_origin center
	billboard_rotation_type vertex
	common_direction 0 1 0
	common_up_vector 0 0 1
	iteration_interval 0
	
	emitter Point
	{
		angle 30
		colour 1 0.3 0 0
		direction 1 0 0
		emission_rate 50
		position 0 0 0
		velocity_min 1
		velocity_max 6
		time_to_live 1
		duration 0.1
	}
	affector Scaler
	{
		rate -0.05
	}
	affector ColourInterpolator
	{
		time0 0
		colour0 1 0.3 0 0
		time1 0.7
		colour1 1 0.3 0 0
		time2 1
		colour2 0 0 0 0
	}
	affector LinearForce{
		force_vector 0 -4 0
		force_application add
	}
	affector DeflectorPlane 
	{
		plane_point 0 4 0
		plane_normal 0 1 0 
		bounce 0.4
	}
}
