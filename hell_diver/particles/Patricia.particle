particle_system patricia_muzzle_flower
{
	quota 4000
    emit_emitter_quota 3500
	material flower_purple
	particle_width 1.0
	particle_height 0.85
	renderer billboard
	billboard_type point
	billboard_origin center
	billboard_rotation_type vertex
	common_direction 0 1 0
	common_up_vector 0 0 1
	iteration_interval 0
	
    emitter Point
    {
        angle 0
		colour 0 0 0 0
		direction 0 1 0
		emission_rate 10
		position 0 1 0
		velocity 0.0001
		time_to_live 1
		duration 0.1
        emit_emitter tag1
    }
	emitter Ring   
	{
        name tag1
		angle 180
		colour 0 0 0 0
		direction 0 1 0
		emission_rate 2000
		position 0 0 0
		velocity_min 0.1
		velocity_max 0.5
		time_to_live 2
		duration 0.5
        width 10
		height 10
		depth 1
		inner_width 1
		inner_height 1
        inner_depth 1
	}
	affector Scaler
	{
		rate -0.3
	}
	affector Rotator
	{
		rotation_speed_range_start	-40
		rotation_speed_range_end	40
		rotation_range_start	0
		rotation_range_end	360
	}
	affector ColourInterpolator
	{
		time0 0
		colour0 1 0.4 1 1
		time1 0.7
		colour1 1 0.4 1 1
		time2 1
		colour2 0 0 0 0
	}
	affector LinearForce{
		force_vector 0 0.5 0
		force_application add
	}
	affector DirectionRandomiser 
	{
		randomness 5
		scope 5
		keep_velocity false
	}
}
particle_system patricia_muzzle_flower_smaller : patricia_muzzle_flower
