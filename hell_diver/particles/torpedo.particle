particle_system torpedo_tail_splash
{
	quota 200
	material water_splash
	particle_width 0.1
	particle_height 0.1
	renderer billboard
	billboard_type perpendicular_common
	billboard_origin center
	billboard_rotation_type vertex
	common_direction 0 1 0
	common_up_vector 0 0 1
	iteration_interval 0
	
	emitter Point
	{
		angle 0
		colour 0 0 0 0
		direction 0 1 0
		emission_rate 30
		position 0 0 0
		velocity_min 1
		velocity_max 3
		time_to_live 0.3
		duration 15
	}
	affector Scaler
	{
		rate 10
	}
	affector Rotator
	{
		rotation_speed_range_start	0
		rotation_speed_range_end	0
		rotation_range_start	0
		rotation_range_end	360
	}
	affector ColourInterpolator
	{
		time0 0
		colour0 1 1 1 0.5
		time1 0.3
		colour1 1 1 1 0.5
		time2 1
		colour2 0 0 0 0
	}
	affector LinearForce{
		force_vector 0 -4 0
		force_application add
	}
}
particle_system torpedo_tail_splash_2 : torpedo_tail_splash
{
    material Tex_Spark28
    emitter Point
	{
		angle 0
		colour 0 0 0 0
		direction 0 1 0
		emission_rate 50
		position 0 0 0
		velocity_min 3
		velocity_max 5
		time_to_live 0.1
		duration 15
	}
	affector Scaler
	{
		rate 30
	}
}
